                            <ul class="wsmenu-list">
                            <li class="nl-simple" aria-haspopup="true"><a href="{{url('app_admin')}}">Calendrier</a></li>
                            
                                <!-- LIEN DE NAVIGATION SIMPLE -->
                                <li class="nl-simple header-btn" aria-haspopup="true"><a href="{{ url('app_logout') }}" class="header-btn btn-primary tra-black-hover last-link">Déconnexion</a></li>
                            </ul>
                        </nav>  <!-- FIN DU MENU PRINCIPAL -->

                    </div>
                </div>  <!-- FIN DU MENU DE NAVIGATION -->

            </div>  <!-- Fin du wrapper d'en-tête -->
        </header>  <!-- FIN DE L'EN-TÊ


        
        

        <!-- MAIN CONTENT -->
        <div class="inner-page-wrapper">
            {% block body %}
            {% endblock %}
        </div> <!-- END MAIN CONTENT -->
    </div> <!-- END PAGE CONTENT -->

    <!-- EXTERNAL SCRIPTS -->    
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <script src="{{ asset('js/jquery-3.3.1.min.js') }}"></script>
    <script src="{{ asset('js/bootstrap.min.js') }}"></script>    
