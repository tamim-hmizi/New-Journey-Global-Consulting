                    <span class="smllogo"><img src="{{ asset('logo/logo-01.png') }}" height="50" alt="mobile-logo"/></span>
                    
                </div>

                

                <!-- MENU DE NAVIGATION -->
                <div class="wsmainfull menu clearfix">
                    <div class="wsmainwp clearfix">

                        <!-- IMAGE DE LOGO -->
                        <div class="desktoplogo"><a href="" class="logo-black"><img src="{{ asset('logo/long.png') }}"  height="70" alt="header-logo"></a></div>
                        {# <div class="desktoplogo"><a href="#hero-1" class="logo-white"><img src="{{ asset('logo/logo-Blanc-01.png.png') }}" height="70" alt="header-logo"></a></div> #}

                        <!-- MENU PRINCIPAL -->
